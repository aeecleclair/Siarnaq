    "src\\components\\admin\\adminPanel\\RecapPanel\\MigrateUserForm.tsx": [
      "Autre",
      "Adoma",
      "Exte"
    ],
    "src\\components\\admin\\userSearch\\Columns.tsx": [" — "],
    "src\\components\\custom\\CurrencyInput.tsx": ["€"],
    "src\\components\\custom\\MultiSelect.tsx": ["Enter"],
    "src\\components\\user\\AssociationPanel.tsx": ["·"]
  }
}
